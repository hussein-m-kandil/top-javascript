    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  },
  "browserslist": [
    "defaults",
    "last 2 versions",
    "not dead",
    "> 0.2%",
    "Firefox ESR"
